        <mxCell id="On5UjnBoXKvAq1PpS9VR-21" value="Verbs" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontStyle=1;fontSize=16;" parent="1" vertex="1">
          <mxGeometry x="344" y="350" width="60" height="30" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-28" value="Mix and match for the rest:&lt;div&gt;&lt;ul&gt;&lt;li&gt;mi i de en manja a oranje - I was eating an orange.&lt;/li&gt;&lt;li&gt;mi i de ave en manja a oranje - I had been eating an orange.&lt;/li&gt;&lt;li&gt;se mi i kan go, mi i da go - If I could go, I would.&lt;/li&gt;&lt;/ul&gt;&lt;/div&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
          <mxGeometry x="274" y="630" width="280" height="140" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-32" value="Adjectives go before the noun. Adverbs follow the verb with o:&lt;br&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;mi i manja o rapi a oranje poma.&lt;/div&gt;&lt;div&gt;I eat quickly an orange apple (the apple has the colour orange).&lt;/div&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
          <mxGeometry x="610" y="430" width="206" height="110" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-37" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;" parent="1" source="On5UjnBoXKvAq1PpS9VR-33" target="On5UjnBoXKvAq1PpS9VR-36" edge="1">
          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-33" value="&lt;p&gt;Personal pronouns before nouns are possessives:&lt;/p&gt;&lt;ul&gt;&lt;li&gt;tu i manja a mi poma - You eat my apple.&lt;/li&gt;&lt;li&gt;tu kaja - Your box.&lt;/li&gt;&lt;/ul&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
          <mxGeometry x="590" y="185" width="216" height="105" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-36" value="You can also use “de”:&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;poma de Tom.&lt;/div&gt;&lt;div&gt;The apple of Tom.&lt;br&gt;&lt;/div&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
          <mxGeometry x="633" y="315" width="130" height="75.5" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-45" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;" parent="1" source="On5UjnBoXKvAq1PpS9VR-38" target="On5UjnBoXKvAq1PpS9VR-39" edge="1">
          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-38" value="&lt;ul&gt;&lt;li&gt;de - from, of, about, out of, made of.&lt;/li&gt;&lt;li&gt;en - in, at, on.&lt;/li&gt;&lt;li&gt;go - to, for, towards.&lt;/li&gt;&lt;li&gt;kon - with.&lt;/li&gt;&lt;li&gt;sama - like, as, than, same.&lt;/li&gt;&lt;/ul&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
          <mxGeometry x="10" y="290" width="250" height="100" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-55" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;" parent="1" source="On5UjnBoXKvAq1PpS9VR-39" target="On5UjnBoXKvAq1PpS9VR-53" edge="1">
          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-39" value="&lt;ul&gt;&lt;li&gt;Tim e kon Tom - Tim is with Tom.&lt;/li&gt;&lt;li&gt;mi e de México - I’m from México.&lt;/li&gt;&lt;li&gt;Tom i labora en akonto-siensa - Tom works in accounting.&lt;/li&gt;&lt;/ul&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
          <mxGeometry x="10" y="430" width="180" height="123" as="geometry" />
        </mxCell>
