        <mxCell id="On5UjnBoXKvAq1PpS9VR-15" value="&lt;div&gt;Present:&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;mi i manja a oranje.&lt;/div&gt;&lt;div&gt;I eat an orange.&lt;/div&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
