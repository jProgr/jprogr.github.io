        <mxCell id="On5UjnBoXKvAq1PpS9VR-48" value="&lt;div&gt;For simple questions just add ?:&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;tu i manja a oranje?&lt;/div&gt;&lt;div&gt;Do you eat oranges?&lt;/div&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
          <mxGeometry x="626" y="580" width="190" height="70" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-50" value="Questions" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontStyle=1;fontSize=16;" parent="1" vertex="1">
          <mxGeometry x="683" y="553" width="60" height="30" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-51" value="&lt;div&gt;For the rest use ke:&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;ul&gt;&lt;li&gt;tu i manja a ke? - What do you eat?&lt;/li&gt;&lt;li&gt;ke man i manja a oranje - Who eats oranges?&lt;/li&gt;&lt;li&gt;tu i manja e ke loke? - Where are you eating?&lt;/li&gt;&lt;li&gt;si i manja en ke tempo? - When is he eating?&lt;/li&gt;&lt;li&gt;tu i manja e ke rason? - Why do you eat?&lt;/li&gt;&lt;li&gt;mi i debe manja a ke mui? - How much should I eat?&lt;/li&gt;&lt;li&gt;si i manja e ke moda? - How does she eat?&lt;/li&gt;&lt;/ul&gt;&lt;/div&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
          <mxGeometry x="573" y="690" width="243" height="265" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-60" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;" parent="1" source="On5UjnBoXKvAq1PpS9VR-53" target="On5UjnBoXKvAq1PpS9VR-57" edge="1">
          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="On5UjnBoXKvAq1PpS9VR-53" value="&lt;ul&gt;&lt;li&gt;nulo - 0.&lt;/li&gt;&lt;li&gt;uno - 1.&lt;/li&gt;&lt;li&gt;duo - 2.&lt;/li&gt;&lt;li&gt;san - 3.&lt;/li&gt;&lt;li&gt;fo - 4.&lt;/li&gt;&lt;li&gt;penta - 5.&lt;/li&gt;&lt;li&gt;sita - 6&lt;/li&gt;&lt;li&gt;seven - 7.&lt;/li&gt;&lt;li&gt;ba - 8.&lt;/li&gt;&lt;li&gt;nin - 9.&lt;/li&gt;&lt;li&gt;ten - 10.&lt;/li&gt;&lt;li&gt;ten nin poma - 19 apples.&lt;/li&gt;&lt;/ul&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;spacing=6;" parent="1" vertex="1">
